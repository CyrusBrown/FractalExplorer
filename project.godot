config_version=5

[application]

config/name="FractalExplorer"
run/main_scene="res://fractal.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Aliases="*res://Aliases.gd"
KeybindManager="*res://KeybindManager.gd"
Persistance="*res://Persistance.gd"

[debug]

settings/stdout/print_fps=true
settings/stdout/print_gpu_profile=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/main_scene="res://fractalgeneral.tscn"

